doctype html
html
    head
        title=AsianSocks
        link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")
    body
        h1 #{title}
        h2 Configuration de l'API twitter
        form(method="post")
            br
            br
            p #{consumerkey}
            label(for='consumerkey') Key Consummer: 
            input(type='text', value=consumerkey, id='consumerkey', name='consumerkey')
            br
            br
            label(for='consumersecret') Secret Consummer: 
            input(type='text', value=consumersecret, id='consumersecret', name='consumersecret')
            br
            br
            label(for='token') Token: 
            input(type='text', value=token, id='token', name='token')
            br
            br
            label(for='tokensecret') Token secret: 
            input(type='text', value=tokensecret, id='tokensecret', name='tokensecret')
            br
            br
            button(type='submit') Envoyer
            br
            br
            br
        h2 Configuration de la BDD
        form(method="post")
            br
            br
            label(for='ip') Adress IP: 
            input(type='text', value=ip, id='ip', name='ip')
            br
            br
            label(for='port') Port: 
            input(type='text', value=port, id='port', name='port')
            br
            br
            label(for='db') DB: 
            input(type='text', value=db, id='db', name='db')
            br
            br
            button(type='submit') Sign up
script(src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js")
script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js")
